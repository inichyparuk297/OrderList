{
	"name": "OrderList",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "npx react-native run-android",
		"android:build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
		"android:build:release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
		"android:install:debug": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug && cd ..",
		"android:clean": "cd android && ./gradlew clean && ./gradlew cleanBuildCache && cd ..",
		"ios": "react-native run-ios",
		"ios:podinstall": "cd ios && pod install && cd ..",
		"ios:build": "react-native bundle --entry-file='index.js' --bundle-output='./ios/pingd/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint .",
		"build": "tsc"
	},
	"dependencies": {
		"@react-native-community/masked-view": "^0.1.6",
		"@react-navigation/native": "^5.0.5",
		"@react-navigation/stack": "^5.0.5",
		"@types/react": "^16.9.19",
		"@types/react-native": "^0.61.15",
		"@types/react-native-material-ui": "^1.32.0",
		"@types/react-redux": "^7.1.7",
		"jetifier": "^1.6.5",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"react": "^16.12.0",
		"react-native": "^0.61.5",
		"react-native-gesture-handler": "^1.6.0",
		"react-native-material-ui": "^1.30.1",
		"react-native-reanimated": "^1.7.0",
		"react-native-safe-area-context": "^0.7.3",
		"react-native-screens": "^2.0.0-beta.4",
		"react-native-vector-icons": "^6.6.0",
		"react-redux": "^7.1.3",
		"redux": "^4.0.5",
		"redux-logger": "^3.0.6",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/runtime": "^7.6.2",
		"@react-native-community/eslint-config": "^0.0.5",
		"@types/jest": "^25.1.2",
		"@types/react-test-renderer": "^16.9.2",
		"babel-jest": "^24.9.0",
		"eslint": "^6.5.1",
		"jest": "^24.9.0",
		"metro-react-native-babel-preset": "^0.56.0",
		"react-native-typescript-transformer": "^1.2.13",
		"react-test-renderer": "16.9.0",
		"ts-jest": "^25.2.0",
		"typescript": "^3.7.5"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(js)$": "/node_modules/babel-jest",
			"\\.(ts|tsx)$": "/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"testPathIgnorePatterns": [
			"\\.snap$",
			"/node_modules/"
		],
		"cacheDirectory": ".jest/cache"
	}
}
